   "source": [
    "# создадим функцию, которая ищет ключевое слово по словарю и возвращает ключ\n",
    "def find_product_type(x):\n",
    "    for key, values in product_dict.items():\n",
    "        for value in values:\n",
    "            if value in x:\n",
    "                return key"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [],
   "source": [
    "# создадим пустую колонку product_type и заполним её категориями товара, применив функцию find_product_type\n",
    "df['product_type'] = ''\n",
    "df['product_type'] = df['product'].apply(find_product_type)\n"
   ]
  },
