   "source": [
    "# Визуализируем количество уникальных пользователей в каждом классе\n",
    "\n",
    "customer_id_by_class = RFM_Segment.reset_index().groupby('Class')['customer_id'].count().reset_index().sort_values(by='customer_id', ascending=False)\n",
    "f, ax = plt.subplots(figsize=(15, 7))\n",
    "sns.barplot(x='Class', y='customer_id', data=customer_id_by_class)\n",
    "plt.title('Количество клиентов по классам')\n",
    "plt.xticks(rotation=45)\n",
    "plt.xlabel('Класс пользователей')\n",
    "plt.ylabel('Количество');"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
